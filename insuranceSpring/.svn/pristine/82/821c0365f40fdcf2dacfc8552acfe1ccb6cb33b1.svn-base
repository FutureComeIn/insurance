package com.dxm.insuranceSpring.pojo;

/**
 * 勘察明细信息实体类
 * @author Administrator
 *
 */
public class CheckDetails {
	private Integer checkDetailsId;
	private Integer checkId;
	private String checkPicture;
	private String checkRemark;
	public Integer getCheckDetailsId() {
		return checkDetailsId;
	}
	public void setCheckDetailsId(Integer checkDetailsId) {
		this.checkDetailsId = checkDetailsId;
	}
	public Integer getCheckId() {
		return checkId;
	}
	public void setCheckId(Integer checkId) {
		this.checkId = checkId;
	}
	public String getCheckPicture() {
		return checkPicture;
	}
	public void setCheckPicture(String checkPicture) {
		this.checkPicture = checkPicture;
	}
	public String getCheckRemark() {
		return checkRemark;
	}
	public void setCheckRemark(String checkRemark) {
		this.checkRemark = checkRemark;
	}
	@Override
	public String toString() {
		return "CheckDetails [checkDetailsId=" + checkDetailsId + ", checkId=" + checkId + ", checkPicture="
				+ checkPicture + ", checkRemark=" + checkRemark + "]";
	}
	
}
