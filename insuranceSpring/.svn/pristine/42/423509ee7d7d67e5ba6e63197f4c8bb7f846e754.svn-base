package com.dxm.insuranceSpring.pojo;

import java.sql.Date;

public class SettleClaim {
	private int settleclaimId;    //理赔编号
	private int caseId;//案件编号
	private int nuclearDamageId;//核损编号
	private int settleclaimPrice;//理赔金额
	private int settleclaimTime;//理赔时间
	private int settleClaimstatus;//理赔状态
	private int userId;//理赔人编号
	private String username;//理赔人
	
	
	public SettleClaim() {
		super();
		
	}


	public SettleClaim(int settleclaimId, int caseId, int nuclearDamageId, int settleclaimPrice, int settleclaimTime,
			int settleClaimstatus, int userId, String username) {
		super();
		this.settleclaimId = settleclaimId;
		this.caseId = caseId;
		this.nuclearDamageId = nuclearDamageId;
		this.settleclaimPrice = settleclaimPrice;
		this.settleclaimTime = settleclaimTime;
		this.settleClaimstatus = settleClaimstatus;
		this.userId = userId;
		this.username = username;
	}


	public int getSettleclaimId() {
		return settleclaimId;
	}


	public void setSettleclaimId(int settleclaimId) {
		this.settleclaimId = settleclaimId;
	}


	public int getCaseId() {
		return caseId;
	}


	public void setCaseId(int caseId) {
		this.caseId = caseId;
	}


	public int getNuclearDamageId() {
		return nuclearDamageId;
	}


	public void setNuclearDamageId(int nuclearDamageId) {
		this.nuclearDamageId = nuclearDamageId;
	}


	public int getSettleclaimPrice() {
		return settleclaimPrice;
	}


	public void setSettleclaimPrice(int settleclaimPrice) {
		this.settleclaimPrice = settleclaimPrice;
	}


	public int getSettleclaimTime() {
		return settleclaimTime;
	}


	public void setSettleclaimTime(int settleclaimTime) {
		this.settleclaimTime = settleclaimTime;
	}


	public int getSettleClaimstatus() {
		return settleClaimstatus;
	}


	public void setSettleClaimstatus(int settleClaimstatus) {
		this.settleClaimstatus = settleClaimstatus;
	}


	public int getUserId() {
		return userId;
	}


	public void setUserId(int userId) {
		this.userId = userId;
	}


	public String getUsername() {
		return username;
	}


	public void setUsername(String username) {
		this.username = username;
	}


	@Override
	public String toString() {
		return "SettleClaim [settleclaimId=" + settleclaimId + ", caseId=" + caseId + ", nuclearDamageId="
				+ nuclearDamageId + ", settleclaimPrice=" + settleclaimPrice + ", settleclaimTime=" + settleclaimTime
				+ ", settleClaimstatus=" + settleClaimstatus + ", userId=" + userId + ", username=" + username + "]";
	}
	
	
	
	
	
	
}
