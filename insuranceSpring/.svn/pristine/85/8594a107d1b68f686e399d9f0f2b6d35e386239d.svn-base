package com.dxm.insuranceSpring.pojo;

/**
 * 案件信息实体类
 * @author Administrator
 *
 */
public class Case {
	private Integer caseId;
	private Integer reportId;
	private String reportReason;
	private String address;
	private Integer jieanId;
	private Integer checkId;
	private Integer assessmentId;
	private Integer nuclearDamageId;
	private Integer settlementId;
	private Integer settleId;
	private Integer caseStatus;
	
	//一对一 报案和案件
	private Report reportCase;
	
	
	public Report getReportCase() {
		return reportCase;
	}
	public void setReportCase(Report reportCase) {
		this.reportCase = reportCase;
	}
	public Integer getCaseId() {
		return caseId;
	}
	public void setCaseId(Integer caseId) {
		this.caseId = caseId;
	}
	public Integer getReportId() {
		return reportId;
	}
	public void setReportId(Integer reportId) {
		this.reportId = reportId;
	}
	public String getReportReason() {
		return reportReason;
	}
	public void setReportReason(String reportReason) {
		this.reportReason = reportReason;
	}
	public String getAddress() {
		return address;
	}
	public void setAddress(String address) {
		this.address = address;
	}
	public Integer getJieanId() {
		return jieanId;
	}
	public void setJieanId(Integer jieanId) {
		this.jieanId = jieanId;
	}
	public Integer getCheckId() {
		return checkId;
	}
	public void setCheckId(Integer checkId) {
		this.checkId = checkId;
	}
	public Integer getAssessmentId() {
		return assessmentId;
	}
	public void setAssessmentId(Integer assessmentId) {
		this.assessmentId = assessmentId;
	}
	public Integer getNuclearDamageId() {
		return nuclearDamageId;
	}
	public void setNuclearDamageId(Integer nuclearDamageId) {
		this.nuclearDamageId = nuclearDamageId;
	}
	public Integer getSettlementId() {
		return settlementId;
	}
	public void setSettlementId(Integer settlementId) {
		this.settlementId = settlementId;
	}
	public Integer getSettleId() {
		return settleId;
	}
	public void setSettleId(Integer settleId) {
		this.settleId = settleId;
	}
	public Integer getCaseStatus() {
		return caseStatus;
	}
	public void setCaseStatus(Integer caseStatus) {
		this.caseStatus = caseStatus;
	}
	@Override
	public String toString() {
		return "Case [caseId=" + caseId + ", reportId=" + reportId + ", reportReason=" + reportReason + ", address="
				+ address + ", jieanId=" + jieanId + ", checkId=" + checkId + ", assessmentId=" + assessmentId
				+ ", nuclearDamageId=" + nuclearDamageId + ", settlementId=" + settlementId + ", settleId=" + settleId
				+ ", caseStatus=" + caseStatus + "]";
	}
	
}
